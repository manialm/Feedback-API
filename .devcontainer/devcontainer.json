{

  "name": "FastAPI Feedback API",
  "dockerComposeFile": "../docker-compose.yml",
  
  "service": "app",
  
  "workspaceFolder": "/workspace",

  "features": {
    "ghcr.io/devcontainers/features/env-file:1": {
      "path": "/workspace/.env.sample"
    }
  },

  "forwardPorts": [8000, 5432],

  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance"
      ]
    }
  }
}
